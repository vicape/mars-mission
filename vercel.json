{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/ai.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/otherApi.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/ai",
      "dest": "/api/ai.js"
    },
    {
      "src": "/api/other-endpoint",
      "dest": "/api/otherApi.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
